<script type="text/javascript">

$(document).ready(function(){
	$('#body').keyup(function(){
		var temp = $('#body').val();
		var pos = temp.indexOf('==========');
		var previous_text; 
		if(pos<0)
		{
			previous_text = "";
		}
		else
		{
			previous_text = temp.slice(pos); 
			temp = temp.slice(0,pos);
		}
		$.ajax({
			url : '/cgi-bin/request_handler.pl',
			data : {
				input_string : temp 
			},
			contentType: "application/x-www-form-urlencoded;charset=UTF-8",
			success : function(responseText){
			//	$('#body').text(responseText);
				var output = temp +"\n"+ responseText +"\n"+ previous_text;	
				$('#response').text(output);
			}
		});
	});

	$('#translate_toggle').change(function(){
		if ($('#translate_toggle').is(":checked"))
		{
  			$('#response').show();
			 $('#body').keyup(function(){
        var temp = $('#body').val();
        var pos = temp.indexOf('==========');
        var previous_text = temp.slice(pos); 
        temp = temp.slice(0,pos);
        $.ajax({
            url : '/cgi-bin/server_test.pl',
            data : {
                input_string : temp 
            },
            contentType: "application/x-www-form-urlencoded;charset=UTF-8",
            success : function(responseText){
            //  $('#body').text(responseText);
                var output = temp +"\n\n"+ responseText +"\n\n"+ previous_text; 
                $('#response').text(output);
            }
        });
    });


		}
		else
		{
			$('#response').hide();
			$('#body').off("keyup");
		}
	});
});
</script>


